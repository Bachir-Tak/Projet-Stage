<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" 
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <icon src="res/icon.png" platform="ios" width="100" height="100" density="mdpi" />
    <platform name="android">
  <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
      <application android:usesCleartextTraffic="true" />
  </edit-config>
  <preference name="AndroidPersistentFileLocation" value="Compatibility" />
<preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />
</platform>
    <name>Gestion Cinq</name>
    <description>Gestion de Facture</description>
    <author email="bachir.benna@outlook.fr" >
        Bachir Benna (Developper) and Zakaria Taleb Essalama (Director)
    </author>
        <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <content src="index.html" />
</widget>
